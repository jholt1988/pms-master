    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "msw": "^2.12.3",
    "playwright": "^1.57.0",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.17",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
