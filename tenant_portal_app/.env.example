# AI Features Configuration
# Copy this file to .env.local and add your API keys

# Feature Flags (true/false)
VITE_FEATURE_RENT_OPTIMIZATION=true
VITE_FEATURE_CHATBOT=true
VITE_FEATURE_SMART_BILL_ENTRY=true
VITE_FEATURE_PREDICTIVE_MAINTENANCE=true

# API Keys
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
VITE_ZILLOW_API_KEY=your_zillow_api_key_here
VITE_RENTOMETER_API_KEY=your_rentometer_api_key_here
VITE_AWS_TEXTRACT_KEY=your_aws_textract_key_here
VITE_ML_SERVICE_KEY=your_ml_service_key_here

# Service Configuration
VITE_ML_SERVICE_URL=http://localhost:5000
VITE_LLM_PROVIDER=mock
VITE_LLM_MODEL=gpt-4o-mini
VITE_LLM_ENDPOINT=
VITE_MARKET_DATA_PROVIDER=mock
VITE_OCR_PROVIDER=mock
VITE_PREDICTION_MODEL=mock

# Service Settings
VITE_AI_SERVICE_TIMEOUT=10000
VITE_CACHE_ENABLED=true
VITE_RENT_CACHE_TTL=86400
VITE_RENT_UPDATE_INTERVAL=7
VITE_CHATBOT_MAX_TOKENS=500
VITE_CHATBOT_TEMPERATURE=0.7
VITE_OCR_CACHE_TTL=3600
VITE_AUTO_APPROVE_THRESHOLD=0.95
VITE_PREDICTION_CACHE_TTL=21600
VITE_PREDICTION_HORIZON=90
VITE_MIN_DATA_POINTS=10
VITE_PREDICTION_UPDATE_FREQ=24

# Notes:
# - All AI features use mock data by default (no API keys required)
# - Set LLM_PROVIDER to 'openai' or 'anthropic' to use real LLM APIs
# - Set MARKET_DATA_PROVIDER to 'zillow' or 'rentometer' for real market data
# - Set OCR_PROVIDER to 'aws-textract' or 'google-vision' for real OCR
# - Set PREDICTION_MODEL to 'xgboost' or 'random-forest' for real ML predictions
